{
    "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["warn", { "endOfLine": "auto", "trailingComma": "none" }],
        "no-unused-vars": "warn",
        "spaced-comment": "off",
        "no-console": "off"
    }
}
